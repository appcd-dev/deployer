apiVersion: app.k8s.io/v1beta1
kind: Application
metadata:
  name: "$name"
  namespace: "$namespace"
  annotations:
    marketplace.cloud.google.com/deploy-info: '{"partner_id": "stackgen-gcp-marketplace", "product_id": "stackgen-enterprise-platform-k8s-v2.endpoints.stackgen-gcp-marketplace.cloud.goog", "partner_name": "StackGen Inc"}'
    kubernetes-engine.cloud.google.com/icon: 
  labels:
    app.kubernetes.io/name: "$name"
spec:
  descriptor:
    type: terraform-runner
    version: "1.0.2"
    notes: |-
      The IP address of the proxy-ingress service is: 
      $(kubectl get ing proxy-ingress -n stackgen -o jsonpath='{.status.loadBalancer.ingress[0].ip}')
  selector:
    matchLabels:
      app.kubernetes.io/name: "$name"
  componentKinds:
    - group: batch/v1
      kind: Job
